<script>
  var socket = io();
  socket.on('connect', function(){
    var token = $.cookie('faManager');
    socket.emit('authenticate', {token: token});
  })

  $(function () {
    $('[data-toggle="popover"]').popover()
  })

  var checkLogin = function() {
    var isLogged = false
    var email = localStorage.getItem("faManager.email")
    if(email != undefined && email != "") {
      isLogged = true;
      $('#login').fadeOut(1000)
      $('#logout').fadeIn(1000)
    } else {
      $('#logout').fadeOut(1000)
      $('#login').fadeIn(1000)
    }
  }
  checkLogin();
  

</script>
